{
  "name": "__MSG_name__",
  "default_locale": "en",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "__MSG_description__",
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "sidePanel",
    "declarativeNetRequestWithHostAccess",
    "contextMenus",
    "storage"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "csp_rules",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "icons": {
    "16": "assets/icon.png",
    "24": "assets/icon.png",
    "32": "assets/icon.png",
    "48": "assets/icon.png",
    "96": "assets/icon.png",
    "128": "assets/icon.png"
  },
  "action": {},
  "side_panel": {
    "default_path": "sidepanel/sidepanel.html"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "source/*",
        "script/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "host_permissions": [
    "*://*/*"
  ]
}
